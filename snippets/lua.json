{
  "Neovim Plugin Complete": {
    "prefix": "nvpc",
    "body": [
      "---@type LazySpec",
      "return {",
      "\t'${1:example/plugin.nvim}',",
      "\tenabled = true,",
      "\tcond = not vim.g.vscode,",
      "\tversion = '${2:1.0}',",
      "\tbranch = '${3:main}',",
      "\tname = '${4:example}',",
      "\tdependencies = {",
      "\t\t'${5:dep_url}',",
      "\t},",
      "\tbuild = ':${6:BuildCommand}',",
      "\tmain = '${7:text}',",
      "\tpriority = ${8:1000},",
      "\tlazy = ${9:true},",
      "\tft = '${10:lua}',",
      "\tevent = '${11:VeryLazy}',",
      "\tcmd = { '${12:ExampleCommand}' },",
      "\tkeys = {",
      "\t\t{",
      "\t\t\t'<leader>${13:ex}',",
      "\t\t\t'<cmd>${14:ExampleCommand}<cr>',",
      "\t\t\tdesc = '${15:Description}',",
      "\t\t}",
      "\t},",
      "\tspec = {${16}},",
      "\topts = {${17}},",
      "\tconfig = function() ${18} end,",
      "\tinit = function() ${19} end,",
      "}"
    ]
  },
  "Neovim Plugin Simple": {
    "prefix": "nvps",
    "body": [
      "---@type LazySpec",
      "return {",
      "\t'${1:example/plugin.nvim}',",
      "\tenabled = true,",
      "\tcond = not vim.g.vscode,",
      "\tevent = '${2:VeryLazy}',",
      "\tkeys = {",
      "\t\t{",
      "\t\t\t'<leader>${3:ex}',",
      "\t\t\t'<cmd>${4:ExampleCommand}<cr>',",
      "\t\t\tdesc = '${5:Description}',",
      "\t\t}",
      "\t},",
      "\topts = {${6}},",
      "}"
    ]
  },
  "Neovim Colorscheme": {
    "prefix": "nvc",
    "body": [
      "---@type LazySpec",
      "return {",
      "\t'${1:example/colorscheme.nvim}',",
      "\tenabled = true,",
      "\tpriority = 1000,",
      "\tlazy = Slivers.colorscheme.get_config(ColorSliver.colorschemes, vim.g.colors_name).name ~= '${2:example}',",
      "\topts = {",
      "\t\ttransparent = vim.g.transparent_bg,${0}",
      "\t}",
      "}"
    ]
  },
  "Neovim Lang": {
    "prefix": "nvl",
    "body": [
      "local M = {}",
      "",
      "M.highlighters = {${0}}",
      "",
      "M.linters = {}",
      "",
      "M.linters_by_ft = {}",
      "",
      "M.formatters = {}",
      "",
      "M.formatters_by_ft = {}",
      "",
      "M.debuggers = {}",
      "",
      "---@param dap table",
      "function M.dap_adapters(dap) end",
      "",
      "M.dap_filetypes = {}",
      "",
      "---@param dap table",
      "function M.dap_configurations(dap) end",
      "",
      "return M"
    ]
  },
  "Metatable": {
    "prefix": "meta",
    "body": ["local M = {}", "", "${0}", "", "return M"]
  }
}
